
.cu-form-group .title {
  min-width: calc(4em + 60rpx);
}

.steps-bottom.cu-steps .cu-item .num::before {
  content: "\e668";
  font-family: 'iconfont';
}

.text-center{
    position: absolute;
    left: 26vw;
}
.pad-left{
    padding-left:50!
}
.indexLeft{
    position: absolute;
    bottom: 10rpx;
    left: 6vw;
    font-size: 32rpx;
}
.indexRight{
    position: absolute;
    bottom: 10rpx;
    right: 6vw;
    font-size: 32rpx;
}
.indexColor{
    color: #c3c
}
.tower-swiper .tower-item {
    transform: scale(calc(0.5 + var(--index) / 10));
    margin-left: calc(var(--left) * 100rpx - 150rpx);
    z-index: var(--index);
}
.page {
  height: 100Vh;
  width: 100vw;
}

.page.show {
  overflow: hidden;
}

.switch-sex::after {
  content: "\e716";
}

.switch-sex::before {
  content: "\e7a9";
}

.switch-music::after {
  content: "\e66a";
}

.switch-music::before {
  content: "\e6db";
}

