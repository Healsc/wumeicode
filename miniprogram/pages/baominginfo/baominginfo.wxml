

<view class='naxinInfo'>
    <view class="cu-form-group margin-top pad-left">
        <view class="title">姓名</view>
        <text class='text-center pad-left-cont'>{{naxinInfo.name}}</text>
    </view>
    <view class="cu-form-group pad-left">
        <view class="title">性别</view>
        <text class='text-center pad-left-cont'>{{naxinInfo.sex}}</text>
    </view>
    <view class="cu-form-group pad-left">
        <view class="title">学号</view>
       <text class='text-center pad-left-cont'>{{naxinInfo.studentNumber}}</text>
    </view>
    <view class="cu-form-group pad-left">
        <view class="title">学院</view>
        <text class='text-center pad-left-cont'>{{naxinInfo.academy}}</text>
    </view>
    <view class="cu-form-group pad-left">
        <view class="title">班级</view>
       <text class='text-center pad-left-cont'>{{naxinInfo.major}}</text>
    </view>
    <view class="cu-form-group pad-left">
        <view class="title">手机号</view>
        <text class='text-center pad-left-cont'>{{naxinInfo.phone}}</text>
    </view>

    <view class="cu-form-group pad-left">
