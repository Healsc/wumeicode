        <text class='text-center pad-left-cont'>{{naxinInfo.department1}}</text>
    </view>
    <view class="cu-form-group pad-left">
        <view class="title">第二意愿部门</view>
        <text class='text-center pad-left-cont'>{{naxinInfo.department2}}</text>
    </view>

    <view class="cu-form-group pad-left">
        <view class="title">是否服从调剂</view>
        <text class='text-center pad-left-cont'>{{naxinInfo.adjust}}</text>
    </view>
