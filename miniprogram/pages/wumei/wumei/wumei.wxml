
<scroll-view scroll-y class="scrollPage">

    <view class="cu-list menu card-menu margin-top-xl margin-bottom-xl shadow-lg radius">
        <view class="cu-item arrow">
            <view  class="content" bindtap='goToClass' hover-class="none">
                <text class="text-grey">课表收集</text>
            </view>
        </view>

      
        <view class="cu-item arrow">
            <view class="content" bindtap='goShowClass' hover-class="none">
                <text class="text-grey">课表展示</text>
            </view>
        </view>
    </view>
    <view class="cu-tabbar-height"></view>
</scroll-view>

