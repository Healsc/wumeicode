
<view wx:for="{{showList}}" wx:key="{{index}}" class='conten-top'>
    <view class='content-nav' >
        <text>{{item._content}}</text>
    </view>
    <view>
        <block wx:for="{{item._fileIds}}" wx:key="{{index}}">
            <image class='show-img'  mode='widthFix' src="{{item}}"></image>
        </block>
    </view>
</view>
<view class='fengexian' wx:if="{{isBGS}}"></view>
<block wx:if="{{isYYT}}">
    <view class=''>
        <form bindsubmit="submit" bindreset="formReset">
            <!-- <input type="text" name="content"  placeholder='内容'></input> -->
            <view class='sub-nav'>
                <input type="text" name="content"  placeholder='请输入内容 例：9.23-9.23水利学院迎新 负责：办公室'></input>
            </view>
            <view>
            <block wx:for="{{images}}" wx:key="{{index}}">
                <image class='show-img'  mode='widthFix' src="{{item}}"></image>
            </block>
            </view>
            <button bindtap='uPLoadImage'>上传图片</button>
            <button form-type="submit">发布</button>
        </form>
    </view>
</block>
