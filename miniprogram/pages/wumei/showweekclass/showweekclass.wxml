
<scroll-view scroll-y class="scrollPage">
    <view class="cu-list menu card-menu margin-top-xl margin-bottom-xl shadow-lg radius" >
        <view class="cu-item arrow"  wx:for="{{weekList}}" wx:key="{{index}}">
        <navigator url='/pages/wumei/showclass/showclass?weekid={{item}}' class="content" hover-class="none">
            <view class="content"  hover-class="none">
                <text class="text-grey">第{{item}}周课表信息</text>
            </view>
        </navigator>
        </view>
    </view>
    <view class="cu-tabbar-height"></view>
</scroll-view>